.`heading` {
	position: relative;
	text-align: center;
	height: 17px;
}

.`body` {
	padding-left: 0;
	padding-right: 0;
}

.`topRightButton` {
	position: absolute;
	right: 2px;
	top: 2px;
}

.`sliderLabel` {
	text-align: center;
	font-size: 12px;
	text-transform: uppercase;
}

.`sliderValueLabel` {
	font-size: 11px;
	width: 40px;
}

.`sliderMinValue` {
	text-align: right;
}

.`sliderMaxValue` {
	text-align: left;
}

