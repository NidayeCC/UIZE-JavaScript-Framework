.`digit` {
	padding: 10px;
}

.`separator` {
	position: relative;
	width: 20px;
	height: 160px;
	padding: 10px 20px;
}

.`separator` .`separatorColon` {
	position: absolute;
	width: 20px;
	height: 20px;
	background: #3f6;
}

.`separator` .`separatorColonTop` {
	top: 30%;
}

.`separator` .`separatorColonBottom` {
	bottom: 30%;
}

