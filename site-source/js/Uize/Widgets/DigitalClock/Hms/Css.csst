.`digit` {
	float: left;
	padding: 10px;
	width: 83px;
}

.`hmsSeparator` {
	position:relative;
	float:left;
	width:20px;
	height:160px;
	padding:10px 20px;
}

.`hmsSeparator` .`hmsSeparatorColon` {
	position:absolute;
	width:20px;
	height:20px;
	background:#3f6;
}

.`hmsSeparator` .`hmsSeparatorColonTop` {
	top:30%;
}

.`hmsSeparator` .`hmsSeparatorColonBottom` {
	bottom:30%;
}

