<%@ required ([
	'Uize.WidgetV2.AnotherTestWidget',
	'Uize.Widget.Bar.Slider.xSkin'
]) %>
<div id="<%= this.nodeId () %>" class="<%= this.cssClass ('selected') %>" style="border:1px solid #f00; padding:10px;">
	TEST WIDGET
	<%= this.childHtml ({
		widgetClass:Uize.WidgetV2.AnotherTestWidget,
		label:'FOO'
	}) %>
	<%= this.childHtml ({
		widgetClass:Uize.Widget.Bar.Slider,
		minValue:0,
		maxValue:100,
		value:90,
		orientation:'horizontal',
		borderThickness:2
	}) %>
</div>

